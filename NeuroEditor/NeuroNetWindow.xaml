<Window x:Class="NeuroEditor.NeuroNetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NeuroEditor"
        mc:Ignorable="d"
        Title="NeuroNetWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="1">
            <Button Name="TeachNet" Width="120" Margin="20" Click="TeachNet_Click">Обучить нейросеть</Button>
        </StackPanel>
    </Grid>
</Window>
